{
  "name": "angular2-lastfm",
  "version": "0.10.0",
  "description": "A Last.fm API service for Angular2",
  "author": "mikeybyker+github@gmail.com",
  "keywords": [
    "angular2",
    "angular2js",
    "last.fm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mikeybyker/angular2-lastfm.git"
  },
  "scripts": {
    "start": "npm run tsc && npm run uglify",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "uglify": "uglifyjs dist/lastfm.service.js -o dist/lastfm.service.min.js"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "systemjs": "0.19.27",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "uglify-js": "^2.7.0"
  }
}